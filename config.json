{
	"description": "pyveth - veth network driver in Python",
	"documentation": "https://github.com/jacekkow/docker-plugin-pyveth",
	"workdir": "/usr/src/app",
	"entrypoint": ["uwsgi", "--ini", "uwsgi.ini"],
	"env": [
		{
			"name": "HOME",
			"value": "/usr/src/app"
		}
	],
	"interface" : {
		"types": ["docker.networkdriver/1.0"],
		"socket": "pyveth.sock"
	},
	"linux": {
		"capabilities": ["CAP_NET_ADMIN"]
	},
	"network": {
		"type": "host"
	}
}
